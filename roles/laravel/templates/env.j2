APP_NAME=Laravel
APP_ENV=production
APP_KEY=
APP_DEBUG=false
APP_URL=https://multibook.cloud.makedara.work

LOG_CHANNEL=stack

DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE={{ laravel_db_name }}
DB_USERNAME={{ laravel_db_user }}
DB_PASSWORD={{ laravel_db_password }}

AWS_ACCESS_KEY_ID={{ aws_access_key }}
AWS_SECRET_ACCESS_KEY={{ aws_secret_key }}
AWS_DEFAULT_REGION=ap-northeast-1
AWS_BUCKET={{ aws_s3_bucket }}

IMG_ENDPOINT=https://multibook-img.cloud.makedara.work

GOOGLE_CLIENT_ID={{ google_client_id }}
GOOGLE_CLIENT_SECRET={{ google_client_secret }}
GOOGLE_REDIRECT_URI={{ google_redirect_uri }}